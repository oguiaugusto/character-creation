{
	"extends": [
		"airbnb",
		"airbnb-typescript"
	],
	"parserOptions": {
	  	"project": "./tsconfig.json"
	},
	"rules": {
		"react/function-component-definition": "off",
		"react/jsx-curly-spacing": [
			2,
			{
				"when": "always",
				"children": true,
				"allowMultiline": true,
				"objectLiterals": "never"
			}
		],
		"import/prefer-default-export": "off",
		"arrow-body-style": "off",
		"object-curly-newline": "off"
	}
}